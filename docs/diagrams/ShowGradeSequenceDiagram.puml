@startuml
!include style.puml

box Ui UI_COLOR_T1
participant ":MainWindow" as MainWindow UI_COLOR
end box

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ModQuikParser" as ModQuikParser LOGIC_COLOR
participant ": SwitchCommandParser" as SwitchCommandParser LOGIC_COLOR
participant "s:SwitchCommand" as SwitchCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":ModelManager" as ModelManager MODEL_COLOR
participant ":ModQuik" as ModQuik MODEL_COLOR
end box

[-> MainWindow : executeCommand("switch f/grade")
activate MainWindow

MainWindow -> LogicManager: execute("switch f/grade")
activate LogicManager

LogicManager -> ModQuikParser : parseCommand("switch f/grade")
activate ModQuikParser

create SwitchCommandParser
ModQuikParser -> SwitchCommandParser
activate SwitchCommandParser

SwitchCommandParser --> ModQuikParser
deactivate SwitchCommandParser

ModQuikParser -> SwitchCommandParser : parse(" f/grade")
activate SwitchCommandParser

create SwitchCommand
SwitchCommandParser -> SwitchCommand
activate SwitchCommand

SwitchCommand --> SwitchCommandParser : s
deactivate SwitchCommand

SwitchCommandParser --> ModQuikParser : s
deactivate SwitchCommandParser
SwitchCommandParser -[hidden]-> ModQuikParser
destroy SwitchCommandParser

ModQuikParser --> LogicManager : s
deactivate ModQuikParser

LogicManager -> SwitchCommand : execute()
activate SwitchCommand

create CommandResult
SwitchCommand -> CommandResult
activate CommandResult

CommandResult --> SwitchCommand
deactivate CommandResult

SwitchCommand --> LogicManager : result
deactivate SwitchCommand

LogicManager --> MainWindow
deactivate LogicManager

MainWindow -> MainWindow : handleShowChart()
activate MainWindow

MainWindow -> ModelManager : getStudentGradeChartData()
activate ModelManager

ModelManager -> ModQuik : getGradeData()
activate ModQuik

ModQuik --> ModelManager
deactivate ModQuik

ModelManager --> MainWindow
deactivate ModelManager
deactivate MainWindow

[<--MainWindow
deactivate MainWindow
@enduml
