<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<VBox styleClass="pane-with-border" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <Button fx:id="sideButton" maxWidth="Infinity" prefWidth="150" prefHeight="60" mnemonicParsing="false">
        <graphic>
            <ImageView fx:id="logo" fitHeight="24.0" fitWidth="24.0" preserveRatio="true"/>
        </graphic>
    </Button>
</VBox>
